<?xml version="1.0"?>
<ruleset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="MySource" xsi:noNamespaceSchemaLocation="./vendor/squizlabs/php_codesniffer/phpcs.xsd">
    <description>Custom standard for this project</description>

    <!-- Include all PSR12 -->
    <rule ref="PSR12">
        <!-- Except camel case method names -->
        <exclude name="PSR1.Methods.CamelCapsMethodName"/>
    </rule>

</ruleset>
